          <camunda:inputParameter name="saveObjectList">${caseRelationList}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0tbyj9n</bpmn:incoming>
