      <bpmn:scriptTask id="Activity_pipelineData" name="Parse pipeline data" scriptFormat="groovy">
        <bpmn:incoming>Flow_0uzcnte</bpmn:incoming>
        <bpmn:outgoing>Flow_13g5anu</bpmn:outgoing>
        <bpmn:script>import static org.camunda.spin.Spin.*
import static org.camunda.spin.DataFormats.*
import com.cognive.projects.casernkb.model.CaseRulesDto
import com.prime.db.rnkb.model.BaseDictionary

boolean isCodeAccepted(String code) {
   def intCode = (code as int)
   return intCode &lt; 34 &amp;&amp; intCode &gt; 2;
}

def payments = [:]
def pipeLineData = execution.getVariable("pipeLineData")

pipeLineData.each{pipeLineItem -&gt;
    def caseRules = payments[pipeLineItem.paymentExId]
    if(caseRules == null) {
        caseRules = new CaseRulesDto()
    }


    def ruleCode = pipeLineItem.uId.code
    if(isCodeAccepted(ruleCode)) {
      caseRules.rules.add(pipeLineItem.uId)
    }

    caseRules.paymentExId = pipeLineItem.paymentExId
    caseRules.caseType = "" // TODO batchRule.prop("CaseType").stringValue()

    payments[pipeLineItem.paymentExId] = caseRules
}


def paymentsList = payments.collect { it.value }

execution.setVariable("payments", paymentsList)
</bpmn:script>
