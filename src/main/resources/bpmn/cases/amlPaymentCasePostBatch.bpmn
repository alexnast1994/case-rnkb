      <bpmn:scriptTask id="Activity_parsePayload" name="Parse payload" scriptFormat="groovy">
        <bpmn:incoming>Flow_1qme100</bpmn:incoming>
        <bpmn:outgoing>Flow_12b0cih</bpmn:outgoing>
        <bpmn:script>import static org.camunda.spin.Spin.*
import static org.camunda.spin.DataFormats.*
import com.cognive.projects.casernkb.model.CaseRulesDto
import com.prime.db.rnkb.model.BaseDictionary

BaseDictionary getRule(String code) {
   baseDictRepo.getByBaseDictionaryTypeCodeAndCharCode(272, code);
}

boolean isCodeAccepted(String code) {
   def bdRule = getRule(code)
   def intCode = (bdRule.code as int)
   return intCode &lt; 34 &amp;&amp; intCode &gt; 2;
}

def jsonStr = execution.getVariable("payload")
def jsonData = JSON(jsonStr)

def payloadObject = jsonData.prop("payload").prop("amlPaymentCasePostBatch")

def batch = payloadObject.prop("operation").elements()
def payments = [:]


// Group by payment
batch.each{batchRule -&gt; 
    def paymentId = batchRule.prop("PaymentId").numberValue().longValue()
    def caseRules = payments[paymentId]
    if(caseRules == null) {
        caseRules = new CaseRulesDto()
    }


    def ruleCode = batchRule.prop("UID").stringValue()
    if(isCodeAccepted(ruleCode)) {
      caseRules.rules.add(ruleCode)
    }

    caseRules.paymentId = paymentId
    caseRules.caseType = batchRule.prop("CaseType").stringValue()


    payments[paymentId] = caseRules
}

def paymentsList = payments.collect { it.value }

execution.setVariable("payments", paymentsList)
</bpmn:script>
      </bpmn:scriptTask>
