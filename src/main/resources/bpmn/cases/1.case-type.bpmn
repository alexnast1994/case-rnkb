    <bpmn:scriptTask id="Activity_0chda3a" name="Установить&#10;Case.Status">
      <bpmn:incoming>Flow_1o6kyhg</bpmn:incoming>
      <bpmn:outgoing>Flow_0ikzvkr</bpmn:outgoing>
      <bpmn:script>import com.prime.db.rnkb.model.Case
import com.prime.db.rnkb.model.BaseDictionary

BaseDictionary getCaseStatus(String code) {
   baseDictRepo.getByBaseDictionaryTypeCodeAndCode(18, code);
}


if(case.getCaseType().getCode() == "1" ||
case.caseType.code == "3") {
case.status = getCaseStatus("9");
} else if(case.getCaseType().getCode() == "2") {
case.status = getCaseStatus("10");
} else if(case.getCaseType().getCode() == "4") {
case.status = getCaseStatus("12");
}

return case</bpmn:script>
