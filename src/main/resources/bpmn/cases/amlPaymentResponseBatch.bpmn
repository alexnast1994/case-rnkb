  <bpmn:process id="amlPaymentResponseBatch" name="Ответ по платежу 2.1 процесс (pipeline) Batch" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1">
      <bpmn:outgoing>Flow_15m1zmu</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:scriptTask id="Activity_parse" name="Parse payload" scriptFormat="groovy">
      <bpmn:incoming>Flow_15m1zmu</bpmn:incoming>
      <bpmn:outgoing>Flow_0tekepu</bpmn:outgoing>
      <bpmn:script>import static org.camunda.spin.Spin.*
import static org.camunda.spin.DataFormats.*
import com.cognive.projects.casernkb.model.CaseRulesDto

def jsonStr = execution.getVariable("payload")
def jsonData = JSON(jsonStr)

def payloadObject = jsonData.prop("payload").prop("amlPaymentResponseBatch")

def batch = payloadObject.prop("operation").elements()
def payments = [:]

// Group by payment
batch.each{batchRule -&gt; 
    def paymentId = batchRule.prop("PaymentId").numberValue()
    def caseRules = payments[paymentId]
    if(caseRules == null) {
        caseRules = new CaseRulesDto()
    }

    caseRules.paymentId = paymentId
    caseRules.caseType = batchRule.prop("CaseType").stringValue()
    caseRules.rules.add(batchRule.prop("UID").stringValue())

    payments[paymentId] = caseRules
}

def paymentsList = payments.collect { it.value }

execution.setVariable("payments", paymentsList)
</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:sequenceFlow id="Flow_15m1zmu" sourceRef="StartEvent_1" targetRef="Activity_parse" />
    <bpmn:scriptTask id="Activity_loopPayment" name="Для каждого payment" scriptFormat="groovy">
      <bpmn:incoming>Flow_04zjd0p</bpmn:incoming>
      <bpmn:incoming>Flow_0tekepu</bpmn:incoming>
      <bpmn:outgoing>Flow_1da6ev0</bpmn:outgoing>
      <bpmn:script>import com.cognive.projects.casernkb.model.CaseRulesDto

def processPayments = execution.getVariable("processPayments")

// First run init proc
if(processPayments == null) {
    processPayments = []
    processPayments.addAll(execution.getVariable("payments"))
}

def processPayment = false

if(processPayments.size() != 0) {
    execution.setVariable("paymentId", processPayments[0].paymentId)
    execution.setVariable("rules", processPayments[0].rules)

    processPayments.remove(0)
    processPayment = true
}

execution.setVariable("processPayment", processPayment)
execution.setVariable("processPayments", processPayments)
</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:exclusiveGateway id="Gateway_007e3ul" default="Flow_0sfmqgy">
      <bpmn:incoming>Flow_1da6ev0</bpmn:incoming>
      <bpmn:outgoing>Flow_0phrwtt</bpmn:outgoing>
      <bpmn:outgoing>Flow_0sfmqgy</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:callActivity id="Activity_processPaymentResponse" name="2.1 процесс, Ответ по платежу" calledElement="paymentResponse">
      <bpmn:extensionElements>
        <camunda:in source="${caseId}" target="caseId" />
        <camunda:in source="${clientId}" target="clientId" />
        <camunda:in source="${dboRequest}" target="dboRequest" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0phrwtt</bpmn:incoming>
      <bpmn:outgoing>Flow_04zjd0p</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:sequenceFlow id="Flow_04zjd0p" sourceRef="Activity_processPaymentResponse" targetRef="Activity_loopPayment" />
    <bpmn:sequenceFlow id="Flow_1da6ev0" sourceRef="Activity_loopPayment" targetRef="Gateway_007e3ul" />
    <bpmn:sequenceFlow id="Flow_0phrwtt" sourceRef="Gateway_007e3ul" targetRef="Activity_processPaymentResponse">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${processPayment}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0tekepu" sourceRef="Activity_parse" targetRef="Activity_loopPayment" />
    <bpmn:endEvent id="Event_end">
      <bpmn:incoming>Flow_0sfmqgy</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0sfmqgy" sourceRef="Gateway_007e3ul" targetRef="Event_end" />
  </bpmn:process>
