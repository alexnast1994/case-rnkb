      <bpmn:scriptTask id="Activity_0xk4607" name="Set variables" scriptFormat="groovy">
        <bpmn:incoming>Flow_1hqyjlh</bpmn:incoming>
        <bpmn:outgoing>Flow_1dnbhxx</bpmn:outgoing>
