      <bpmn:outgoing>Flow_0t9dnnd</bpmn:outgoing>
      <bpmn:multiInstanceLoopCharacteristics isSequential="true" camunda:collection="payments" camunda:elementVariable="paymentItem" />
      <bpmn:startEvent id="Event_1r4hgtt">
